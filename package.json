{
  "name": "synapsis-apoyos",
  "version": "1.0.0",
  "description": "Sistema de gestiÃ³n de apoyos estudiantiles",
  "main": "webpack.config.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --mode=production",
    "build:dev": "cross-env NODE_ENV=development webpack --mode=development",
    "watch": "cross-env NODE_ENV=development webpack --mode=development --watch",
    "dev": "cross-env NODE_ENV=development webpack serve --mode=development",
    "analyze": "cross-env NODE_ENV=production webpack-bundle-analyzer app/static/dist/stats.json",
    "clean": "rimraf app/static/dist",
    "lint:js": "eslint app/static/src/js/**/*.js",
    "lint:css": "stylelint app/static/src/css/**/*.css",
    "optimize:images": "imagemin app/static/src/images/**/* --out-dir=app/static/dist/images",
    "test": "jest",
    "precommit": "lint-staged"
  },
  "keywords": [
    "flask",
    "education",
    "student-support",
    "management-system"
  ],
  "author": "Synapsis Team",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/plugin-proposal-class-properties": "^7.18.0",
