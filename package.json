    "html-webpack-plugin": "^5.5.0",
    "image-minimizer-webpack-plugin": "^3.8.0",
    "imagemin": "^8.0.0",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.0",
    "imagemin-svgo": "^10.0.0",
