    "webpack-cli": "^5.1.0",
    "webpack-dev-server": "^4.15.0",
    "webpack-manifest-plugin": "^5.0.0"
  },
  "dependencies": {
    "bootstrap": "^5.3.0",
    "chart.js": "^4.3.0",
    "datatables.net": "^1.13.0",
    "datatables.net-bs5": "^1.13.0",
    "jquery": "^3.7.0",
    "select2": "^4.1.0",
    "sweetalert2": "^11.7.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
